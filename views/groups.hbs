<h3>User Groups</h3>

<table>
    <tbody>
    {{#each groups}}
        <tr>
            {{#each this}}
                <tr>
                    <td>{{@key}}</td>
                    <td>{{this}}</td>
                </tr>
            {{/each}}
        </tr>
    {{/each}}
    </tbody>
</table>


<br>
<a href="/">Go back</a>


